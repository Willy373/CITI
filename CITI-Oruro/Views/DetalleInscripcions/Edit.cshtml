@model CITI_Oruro.Models.DetalleInscripcion

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar Registro de Inscripcion</h1>

<h4>Detalle Inscripcion</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdDetalleInscripcion" />
            <div class="form-group">
                <label asp-for="Fecha" class="control-label"></label>
                <input asp-for="Fecha" class="form-control" />
                <span asp-validation-for="Fecha" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <label asp-for="IdIngeniero" class="control-label"></label>
                <select asp-for="IdIngeniero" class="form-control" asp-items="ViewBag.IdIngeniero"></select>
                <span asp-validation-for="IdIngeniero" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdInscripcion" class="control-label"></label>
                <select asp-for="IdInscripcion" class="form-control" asp-items="ViewBag.IdInscripcion"></select>
                <span asp-validation-for="IdInscripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="GUARDAR" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-warning">REGRESAR</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
